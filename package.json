  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "user-agent",
    "browser",
    "http"
  ],
  "license": "MIT",
  "homepage": "https://github.com/FunTW/n8n-nodes-user-agent-random",
  "author": {
    "name": "FunTW",
    "email": "funtw@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FunTW/n8n-nodes-user-agent-random.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
